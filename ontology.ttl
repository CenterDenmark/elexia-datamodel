#################################################################
#    Object Properties
#################################################################

###  http://www.iec.ch/TC57/CIM#ActivePower.multiplier
cim:ActivePower.multiplier rdf:type owl:ObjectProperty ;
                           rdfs:domain cim:ActivePower ;
                           rdfs:range cim:UnitMultiplier .


###  http://www.iec.ch/TC57/CIM#ActivePower.unit
cim:ActivePower.unit rdf:type owl:ObjectProperty ;
                     rdfs:domain cim:ActivePower ;
                     rdfs:range cim:UnitSymbol .


###  http://www.iec.ch/TC57/CIM#Equipment.UsagePoint
cim:Equipment.UsagePoint rdf:type owl:ObjectProperty ;
                         owl:inverseOf :hasUsagePoint ;
                         rdfs:domain cim:UsagePoint ;
                         rdfs:range saref:Device ;
                         rdfs:comment """All devices connecting this usage point to the electrical grid.

Altered compared to the CIM definition, which uses equipment"""@en ;
                         rdfs:label "has equipment" .


###  http://www.iec.ch/TC57/CIM#GeneratingUnit.efficiency
cim:GeneratingUnit.efficiency rdf:type owl:ObjectProperty ;
                              rdfs:domain cim:GeneratingUnit ;
                              rdfs:range cim:PerCent ;
                              rdfs:comment "The efficiency of the unit in converting mechanical energy, from the prime mover, into electrical energy." .


###  http://www.iec.ch/TC57/CIM#GeneratingUnit.maxOperatingP
cim:GeneratingUnit.maxOperatingP rdf:type owl:ObjectProperty ;
                                 rdfs:domain cim:GeneratingUnit ;
                                 rdfs:range cim:ActivePower ;
                                 rdfs:comment "This is the maximum operating active power limit the dispatcher can enter for this unit." .


###  http://www.iec.ch/TC57/CIM#GeneratingUnit.maximumAllowableSpinningReserve
cim:GeneratingUnit.maximumAllowableSpinningReserve rdf:type owl:ObjectProperty ;
                                                   rdfs:domain cim:GeneratingUnit ;
                                                   rdfs:range cim:ActivePower ;
                                                   rdfs:comment "Maximum allowable spinning reserve. Spinning reserve will never be considered greater than this value regardless of the current operating point." .


###  http://www.iec.ch/TC57/CIM#GeneratingUnit.minEconomicP
cim:GeneratingUnit.minEconomicP rdf:type owl:ObjectProperty ;
                                rdfs:domain cim:GeneratingUnit ;
                                rdfs:range cim:ActivePower ;
                                rdfs:comment "Low economic active power limit that must be greater than or equal to the minimum operating active power limit." .


###  http://www.iec.ch/TC57/CIM#GeneratingUnit.minOperatingP
cim:GeneratingUnit.minOperatingP rdf:type owl:ObjectProperty ;
                                 rdfs:domain cim:GeneratingUnit ;
                                 rdfs:range cim:ActivePower ;
                                 rdfs:comment "This is the minimum operating active power limit the dispatcher can enter for this unit." .


###  http://www.iec.ch/TC57/CIM#GeneratingUnit.minimumOffTime
cim:GeneratingUnit.minimumOffTime rdf:type owl:ObjectProperty ;
                                  rdfs:domain cim:GeneratingUnit ;
                                  rdfs:range time:Duration ;
                                  rdfs:comment "Minimum time interval between unit shutdown and startup." .


###  http://www.iec.ch/TC57/CIM#GeneratingUnit.ratedGrossMaxP
cim:GeneratingUnit.ratedGrossMaxP rdf:type owl:ObjectProperty ;
                                  rdfs:domain cim:GeneratingUnit ;
                                  rdfs:range cim:ActivePower ;
                                  rdfs:comment "The unit's gross rated maximum capacity (book value)." .


###  http://www.iec.ch/TC57/CIM#GeneratingUnit.ratedGrossMinP
cim:GeneratingUnit.ratedGrossMinP rdf:type owl:ObjectProperty ;
                                  rdfs:domain cim:GeneratingUnit ;
                                  rdfs:range cim:ActivePower ;
                                  rdfs:comment "The gross rated minimum generation level which the unit can safely operate at while delivering power to the transmission grid." .


###  http://www.iec.ch/TC57/CIM#GeneratingUnit.ratedNetMaxP
cim:GeneratingUnit.ratedNetMaxP rdf:type owl:ObjectProperty ;
                                rdfs:domain cim:GeneratingUnit ;
                                rdfs:range cim:ActivePower ;
                                rdfs:comment "The net rated maximum capacity determined by subtracting the auxiliary power used to operate the internal plant machinery from the rated gross maximum capacity." .


###  http://www.iec.ch/TC57/CIM#GeneratingUnit.startupCost
cim:GeneratingUnit.startupCost rdf:type owl:ObjectProperty ;
                               rdfs:domain cim:GeneratingUnit ;
                               rdfs:range cim:Money ;
                               rdfs:comment "The initial startup cost incurred for each start of the GeneratingUnit." .


###  http://www.iec.ch/TC57/CIM#GeneratingUnit.startupTime
cim:GeneratingUnit.startupTime rdf:type owl:ObjectProperty ;
                               rdfs:domain cim:GeneratingUnit ;
                               rdfs:range time:Duration ;
                               rdfs:comment "Time it takes to get the unit on-line, from the time that the prime mover mechanical power is applied." .


###  http://www.iec.ch/TC57/CIM#GeneratingUnit.totalEfficiency
cim:GeneratingUnit.totalEfficiency rdf:type owl:ObjectProperty ;
                                   rdfs:domain cim:GeneratingUnit ;
                                   rdfs:range cim:PerCent ;
                                   rdfs:comment "The efficiency of the unit in converting the fuel into electrical energy." .


###  http://www.iec.ch/TC57/CIM#GeneratingUnit.variableCost
cim:GeneratingUnit.variableCost rdf:type owl:ObjectProperty ;
                                rdfs:domain cim:GeneratingUnit ;
                                rdfs:range cim:Money ;
                                rdfs:comment "The variable cost component of production per unit of ActivePower." .


###  http://www.iec.ch/TC57/CIM#Location.mainAddress
cim:Location.mainAddress rdf:type owl:ObjectProperty ;
                         rdfs:domain cim:Location ;
                         rdfs:range cim:StreetAddress ;
                         rdfs:comment "Main address of the location." .


###  http://www.iec.ch/TC57/CIM#Location.phone1
cim:Location.phone1 rdf:type owl:ObjectProperty ;
                    rdfs:domain cim:Location ;
                    rdfs:range cim:TelephoneNumber ;
                    rdfs:comment "Phone number." .


###  http://www.iec.ch/TC57/CIM#Location.phone2
cim:Location.phone2 rdf:type owl:ObjectProperty ;
                    rdfs:domain cim:Location ;
                    rdfs:range cim:TelephoneNumber ;
                    rdfs:comment "Additional phone number." .


###  http://www.iec.ch/TC57/CIM#Location.secondaryAddress
cim:Location.secondaryAddress rdf:type owl:ObjectProperty ;
                              rdfs:domain cim:Location ;
                              rdfs:range cim:StreetAddress ;
                              rdfs:comment "Secondary address of the location. For example, PO Box address may have different ZIP code than that in the 'mainAddress'." .


###  http://www.iec.ch/TC57/CIM#Organisation.phone1
cim:Organisation.phone1 rdf:type owl:ObjectProperty ;
                        rdfs:domain cim:Organisation ;
                        rdfs:range cim:TelephoneNumber ;
                        rdfs:comment "Phone number." .


###  http://www.iec.ch/TC57/CIM#Organisation.phone2
cim:Organisation.phone2 rdf:type owl:ObjectProperty ;
                        rdfs:domain cim:Organisation ;
                        rdfs:range cim:TelephoneNumber ;
                        rdfs:comment "Additional phone number." .


###  http://www.iec.ch/TC57/CIM#Organisation.postalAddress
cim:Organisation.postalAddress rdf:type owl:ObjectProperty ;
                               rdfs:domain cim:Organisation ;
                               rdfs:range cim:StreetAddress ;
                               rdfs:comment "Postal address, potentially different than 'streetAddress' (e.g., another city)." .


###  http://www.iec.ch/TC57/CIM#Organisation.streetAddress
cim:Organisation.streetAddress rdf:type owl:ObjectProperty ;
                               rdfs:domain cim:Organisation ;
                               rdfs:range cim:StreetAddress ;
                               rdfs:comment "Street address." .


###  http://www.iec.ch/TC57/CIM#Period.TimeSeries
cim:Period.TimeSeries rdf:type owl:ObjectProperty ;
                      rdfs:domain cim:TimeSeries ;
                      rdfs:range cim:Period ;
                      rdfs:comment "" .


###  http://www.iec.ch/TC57/CIM#Period.resolution
cim:Period.resolution rdf:type owl:ObjectProperty ;
                      rdfs:domain cim:Period ;
                      rdfs:range time:TemporalDuration ;
                      rdfs:comment "The number of units of time that compose an individual step within a period." .


###  http://www.iec.ch/TC57/CIM#Period.timeInterval
cim:Period.timeInterval rdf:type owl:ObjectProperty ;
                        rdfs:domain cim:Period ;
                        rdfs:range time:Interval ;
                        rdfs:comment "The start and end date and time for a given interval." .


###  http://www.iec.ch/TC57/CIM#PositionPoint.Location
cim:PositionPoint.Location rdf:type owl:ObjectProperty ;
                           rdfs:domain cim:Location ,
                                       saref:Device ;
                           rdfs:range cim:PositionPoint ;
                           rdfs:comment "This is only included to empasize the possibility of representing af position as a point in a coordinate system. For geographical location use Geometry"@en ,
                                        "Location described by this position point.Sequence of position points describing this location, expressed in coordinate system 'Location.CoordinateSystem'." ;
                           rdfs:label "has position point"@en .


###  http://www.iec.ch/TC57/CIM#PowerSystemResource.Location
cim:PowerSystemResource.Location rdf:type owl:ObjectProperty ;
                                 owl:inverseOf :hasLocation ;
                                 rdfs:domain cim:Location ;
                                 rdfs:range [ rdf:type owl:Class ;
                                              owl:unionOf ( cim:GeneratingUnit
                                                            cim:PowerSystemResource
                                                            saref:Device
                                                            s4bldg:Building
                                                          )
                                            ] ;
                                 rdfs:comment "Location of this power system resource.All power system resources at this location." ;
                                 rdfs:label "is location of"@en .


###  http://www.iec.ch/TC57/CIM#StreetAddress.streetDetail
cim:StreetAddress.streetDetail rdf:type owl:ObjectProperty ;
                               rdfs:domain cim:StreetAddress ;
                               rdfs:range cim:StreetDetail ;
                               rdfs:comment "Street detail." .


###  http://www.iec.ch/TC57/CIM#StreetAddress.townDetail
cim:StreetAddress.townDetail rdf:type owl:ObjectProperty ;
                             rdfs:domain cim:StreetAddress ;
                             rdfs:range cim:TownDetail ;
                             rdfs:comment "Town detail." .


###  http://www.iec.ch/TC57/CIM#Substation.SubGeographicalRegion
cim:Substation.SubGeographicalRegion rdf:type owl:ObjectProperty ;
                                     rdfs:comment "The SubGeographicalRegion containing the substation.The substations in this sub-geographical region." .


###  http://www.iec.ch/TC57/CIM#TimeSeries.Quantity
cim:TimeSeries.Quantity rdf:type owl:ObjectProperty ;
                        rdfs:domain cim:Quantity ;
                        rdfs:range cim:TimeSeries ;
                        rdfs:comment "" .


###  http://www.iec.ch/TC57/CIM#UsagePoint.CustomerAgreement
cim:UsagePoint.CustomerAgreement rdf:type owl:ObjectProperty ;
                                 rdfs:domain cim:CustomerAgreement ;
                                 rdfs:range cim:UsagePoint ;
                                 rdfs:comment "Customer agreement regulating this service delivery point.All service delivery points regulated by this customer agreement." .


###  http://www.iec.ch/TC57/CIM#UsagePoint.MetrologyRequirement
cim:UsagePoint.MetrologyRequirement rdf:type owl:ObjectProperty ;
                                    rdfs:range cim:UsagePoint ;
                                    rdfs:comment "All metrology requirements for this usage point.All usage points having this metrology requirement." .


###  http://www.iec.ch/TC57/CIM#UsagePoint.Outage
cim:UsagePoint.Outage rdf:type owl:ObjectProperty ;
                      rdfs:domain cim:Outage ;
                      rdfs:range cim:UsagePoint ;
                      rdfs:comment "All outages at this usage point.All usage points associated with this outage." .


###  http://www.iec.ch/TC57/CIM#UsagePoint.PricingStructure
cim:UsagePoint.PricingStructure rdf:type owl:ObjectProperty ;
                                rdfs:domain cim:PricingStructure ;
                                rdfs:range cim:UsagePoint ;
                                rdfs:comment "All pricing structures applicable to this service delivery point (with prepayment meter running as a stand-alone device, with no CustomerAgreement or Customer).All service delivery points (with prepayment meter running as a stand-alone device, with no CustomerAgreement or Customer) to which this pricing structure applies." .


###  http://www.iec.ch/TC57/CIM#UsagePoint.ServiceLocation
cim:UsagePoint.ServiceLocation rdf:type owl:ObjectProperty ;
                               rdfs:domain cim:ServiceLocation ;
                               rdfs:range cim:UsagePoint ;
                               rdfs:comment "Service location where the service delivered by this usage point is consumed.All usage points delivering service (of the same type) to this service location." .


###  http://www.iec.ch/TC57/CIM#UsagePoint.ServiceSupplier
cim:UsagePoint.ServiceSupplier rdf:type owl:ObjectProperty ;
                               rdfs:domain cim:ServiceSupplier ;
                               rdfs:range cim:UsagePoint ;
                               rdfs:comment "ServiceSupplier (utility) utilising this usage point to deliver a service.All usage points this service supplier utilises to deliver a service." .


###  http://www.iec.ch/TC57/CIM#UsagePoint.UsagePointGroup
cim:UsagePoint.UsagePointGroup rdf:type owl:ObjectProperty ;
                               rdfs:domain cim:UsagePointGroup ;
                               rdfs:range cim:UsagePoint ;
                               rdfs:comment "All groups to which this usage point belongs.All usage points in this group." .


###  http://www.iec.ch/TC57/CIM#UsagePoint.UsagePointLocation
cim:UsagePoint.UsagePointLocation rdf:type owl:ObjectProperty ;
                                  rdfs:domain cim:UsagePointLocation ;
                                  rdfs:range cim:UsagePoint ;
                                  rdfs:comment "Location of this usage point.All usage points at this location." .


###  http://www.iec.ch/TC57/CIM#UsagePoint.amiBillingReady
cim:UsagePoint.amiBillingReady rdf:type owl:ObjectProperty ;
                               rdfs:domain cim:UsagePoint ;
                               rdfs:range cim:AmiBillingReadyKind ;
                               rdfs:comment "Tracks the lifecycle of the metering installation at a usage point with respect to readiness for billing via advanced metering infrastructure reads." .


###  http://www.iec.ch/TC57/CIM#UsagePoint.connectionState
cim:UsagePoint.connectionState rdf:type owl:ObjectProperty ;
                               rdfs:domain cim:UsagePoint ;
                               rdfs:range cim:UsagePointConnectedKind ;
                               rdfs:comment "State of the usage point with respect to connection to the network." .


###  http://www.iec.ch/TC57/CIM#UsagePoint.estimatedLoad
cim:UsagePoint.estimatedLoad rdf:type owl:ObjectProperty ;
                             rdfs:domain cim:UsagePoint ;
                             rdfs:range cim:CurrentFlow ;
                             rdfs:comment "Estimated load." .


###  http://www.iec.ch/TC57/CIM#UsagePoint.nominalServiceVoltage
cim:UsagePoint.nominalServiceVoltage rdf:type owl:ObjectProperty ;
                                     rdfs:domain cim:UsagePoint ;
                                     rdfs:range cim:Voltage ;
                                     rdfs:comment "Nominal service voltage." .


###  http://www.iec.ch/TC57/CIM#UsagePoint.phaseCode
cim:UsagePoint.phaseCode rdf:type owl:ObjectProperty ;
                         rdfs:domain cim:UsagePoint ;
                         rdfs:range cim:PhaseCode ;
                         rdfs:comment "Phase code. Number of wires and specific nominal phases can be deduced from enumeration literal values. For example, ABCN is three-phase, four-wire, s12n (splitSecondary12N) is single-phase, three-wire, and s1n and s2n are single-phase, two-wire." .


###  http://www.iec.ch/TC57/CIM#UsagePoint.ratedCurrent
cim:UsagePoint.ratedCurrent rdf:type owl:ObjectProperty ;
                            rdfs:domain cim:UsagePoint ;
                            rdfs:range cim:CurrentFlow ;
                            rdfs:comment "Current flow that this usage point is configured to deliver." .


###  http://www.iec.ch/TC57/CIM#UsagePoint.ratedPower
cim:UsagePoint.ratedPower rdf:type owl:ObjectProperty ;
                          rdfs:domain cim:UsagePoint ;
                          rdfs:range cim:ActivePower ;
                          rdfs:comment "Active power that this usage point is configured to deliver." .


###  http://www.opengis.net/ont/geosparql#hasGeometry
geosp:hasGeometry rdf:type owl:ObjectProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( cim:Location
                                              saref:FeatureOfInterest
                                            )
                              ] ;
                  rdfs:range geosp:Geometry ;
                  rdfs:isDefinedBy """http://www.opengis.net/spec/geosparql/1.0/req/geometry-extension/feature-properties ,
		http://www.opengis.net/spec/geosparql/1.1/req/geometry-extension/feature-properties""" ;
                  skos:definition "A spatial representation for a given FeatureOfInterest or Location."@en ;
                  skos:example """spec11:C.1.1.2.2 ,
		spec11:C.1.1.2.3 ,
		spec11:C.1.1.2.4 ,
		spec11:C.1.1.2.5 ,
		spec11:C.1.1.2.6 ,
		spec11:C.1.1.2.8 ,
		spec11:C.1.1.3.2 ,
		spec11:C.1.1.3.3 ,
		spec11:C.1.2.2 ,
		spec11:C.1.2.3 ,
		spec11:C.1.2.4 ,
		spec11:C.2 """ ;
                  skos:prefLabel "has geometry"@en .


###  http://www.w3.org/2000/01/rdf-schema#member
rdfs:member rdf:type owl:ObjectProperty ;
            rdfs:isDefinedBy "https://www.w3.org/TR/rdf-schema/#ch_member" .


###  http://www.w3.org/2004/02/skos/core#broader
skos:broader rdf:type owl:ObjectProperty ;
             owl:inverseOf skos:narrower .


###  http://www.w3.org/2004/02/skos/core#narrower
skos:narrower rdf:type owl:ObjectProperty .


###  http://www.w3.org/2006/time#hasBeginning
time:hasBeginning rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf time:hasTime ;
                  rdfs:domain time:Interval ;
                  rdfs:range time:Instant ;
                  rdfs:comment "Beginning of a temporal entity"@en ;
                  rdfs:label "has beginning"@en ;
                  skos:definition "Beginning of a temporal entity."@en .


###  http://www.w3.org/2006/time#hasEnd
time:hasEnd rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf time:hasTime ;
            rdfs:domain time:Interval ;
            rdfs:range time:Instant ;
            rdfs:comment "End of a temporal entity."@en ;
            rdfs:label "has end"@en ;
            skos:definition "End of a temporal entity."@en .


###  http://www.w3.org/2006/time#hasTemporalDuration
time:hasTemporalDuration rdf:type owl:ObjectProperty ;
                         rdfs:domain time:TemporalEntity ;
                         rdfs:range time:TemporalDuration ;
                         rdfs:comment "Duration of a temporal entity."@en ;
                         rdfs:label "has temporal duration"@en ;
                         skos:definition "Duration of a temporal entity."@en .


###  http://www.w3.org/2006/time#hasTime
time:hasTime rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( cim:Quantity
                                         saref:ProcedureExecution
                                         :Forecast
                                       )
                         ] ;
             rdfs:range time:TemporalEntity ;
             rdfs:comment "Supports the association of a temporal entity (instant or interval) to any thing"@en ;
             rdfs:label "has time"@en ;
             skos:definition "Supports the association of a temporal entity (instant or interval) to any thing"@en ;
             skos:editorialNote "Feature at risk - added in 2017 revision, and not yet widely used. "@en .


###  http://www.w3.org/2006/time#unitType
time:unitType rdf:type owl:ObjectProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( time:Duration
                                        )
                          ] ;
              rdfs:range time:TemporalUnit ;
              rdfs:comment "The temporal unit which provides the precision of a date-time value or scale of a temporal extent"@en ;
              rdfs:label "temporal unit type"@en .


###  https://saref.etsi.org/core#accomplishes
saref:accomplishes rdf:type owl:ObjectProperty ;
                   owl:inverseOf saref:isAccomplishedBy ;
                   rdfs:range saref:Task ;
                   rdfs:comment "Links a certain entity (e.g., a device) to the task it accomplishes."@en ;
                   rdfs:label "accomplishes"@en .


###  https://saref.etsi.org/core#actsUpon
saref:actsUpon rdf:type owl:ObjectProperty ;
               owl:inverseOf saref:isActedUponBy ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( saref:Command
